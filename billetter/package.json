{
  "name": "billetter",
  "version": "1.0.0",
  "description": "Billetter API - Ticket sales and reservation management",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "test": "vitest run tests/integration",
    "test:watch": "vitest tests/integration",
    "test:integration": "vitest run tests/integration",
    "test:integration:watch": "vitest tests/integration",
    "test:stress": "node scripts/run-stress-tests.js",
    "test:stress-quick": "vitest run --config=vitest.stress.config.js tests/stress/stress-concurrent-booking.test.js --testTimeout=120000",
    "test:stress:concurrent": "vitest run --config=vitest.stress.config.js tests/stress/stress-concurrent-booking.test.js",
    "test:stress:large-events": "vitest run --config=vitest.stress.config.js tests/stress/stress-large-events.test.js",
    "test:stress:payment-spike": "vitest run --config=vitest.stress.config.js tests/stress/stress-payment-spike.test.js",
    "test:stress:sellability": "vitest run --config=vitest.stress.config.js tests/stress/stress-ticket-sellability.test.js",
    "test:stress:comprehensive": "vitest run --config=vitest.stress.config.js tests/stress/stress-comprehensive.test.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js"
  },
  "keywords": [
    "api",
    "tickets",
    "booking",
    "fastify"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "fastify": "^5.5.0"
  }
}
